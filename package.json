{
  "name": "js-given",
  "version": "0.0.8",
  "description": "JavaScript frontend to jgiven",
  "main": "index.js",
  "scripts": {
    "postinstall": "node_modules/protractor/bin/webdriver-manager update",
    "lint": "check-node-version -q --node '< 5.0' || (echo \"Don't worry, ESLint takes some time ...\" && eslint . ./)",
    "lintFix": "check-node-version -q --node '< 5.0' || (echo \"Don't worry, ESLint takes some time ...\" && eslint --fix . ./)",
    "pretest": "npm run flow && npm run lint",
    "test": "npm run test_jest && npm run test_mocha && npm run test_jasmine && npm run test_ava && npm run test_protractor",
    "posttest": "npm run generateReport && npm run build",
    "test_mocha": "mocha --opts spec/support/mocha-default.opts spec",
    "test_jest": "jest --config .jest.conf.json --colors",
    "test_jasmine": "node spec/support/jasmine-runner.js",
    "test_ava": "ava spec/**.spec.js -v",
    "test_protractor": "check-node-version -q --node '< 4.2' || protractor .protractor.conf.js",
    "testWatch": "npm run test_jest -- --watch",
    "flow": "node_modules/flow-bin/cli.js",
    "generateReport": "node src/generateReport.es5.js",
    "__doBuild": "npm run buildLib && npm run buildGenerateReport",
    "build": "check-node-version -q --node '< 5.0' || npm run __doBuild",
    "buildLib": "check-node-version -q --node '< 5.0' || rollup -c rollup/lib.rollup.config.js",
    "buildGenerateReport": "check-node-version -q --node '< 5.0' || rollup -c rollup/generateReport.rollup.config.js",
    "__doPublish": "node preparePublish.js && (cd dist; npm publish)",
    "release": "check-node-version -q --node '< 5.0' || npm test && npm run build && npm run __doPublish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jsGiven/jsGiven.git"
  },
  "keywords": [
    "testing",
    "bdd",
    "jgiven",
    "tests",
    "given",
    "when",
    "then",
    "test"
  ],
  "author": "Florent Le Gall",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jsGiven/jsGiven/issues"
  },
  "homepage": "https://github.com/jsGiven/jsGiven",
  "devDependencies": {
    "ava": "0.16.0",
    "babel-core": "6.17.0",
    "babel-eslint": "7.0.0",
    "babel-jest": "16.0.0",
    "babel-plugin-transform-class-properties": "6.16.0",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-plugin-transform-flow-strip-types": "6.14.0",
    "babel-plugin-transform-object-rest-spread": "6.16.0",
    "babel-plugin-transform-regenerator": "6.16.1",
    "babel-preset-es2015": "6.16.0",
    "babel-preset-es2015-rollup": "^1.2.0",
    "babel-preset-react": "6.16.0",
    "chai": "3.5.0",
    "check-node-version": "1.1.2",
    "eslint": "3.7.1",
    "eslint-config-standard": "6.2.0",
    "eslint-loader": "1.5.0",
    "eslint-plugin-babel": "3.3.0",
    "eslint-plugin-flowtype": "2.20.0",
    "eslint-plugin-import": "2.0.0",
    "eslint-plugin-promise": "2.0.1",
    "eslint-plugin-standard": "2.0.1",
    "flow-bin": "0.33.0",
    "jasmine": "2.5.2",
    "jasmine-spec-reporter": "2.7.0",
    "jest-cli": "16.0.1",
    "mocha": "3.1.0",
    "nodegit": "0.16.0",
    "protractor": "4.0.9",
    "rollup": "0.36.1",
    "rollup-plugin-babel": "^2.6.1",
    "rollup-plugin-uglify": "1.0.1",
    "sinon": "1.17.6",
    "sinon-chai": "2.8.0",
    "tmp": "0.0.29"
  },
  "dependencies": {
    "babel-polyfill": "6.16.0",
    "decompress-zip": "0.3.0",
    "lodash": "4.16.4",
    "machinepack-zip": "1.1.0",
    "maven": "4.0.3",
    "rimraf": "2.5.4",
    "string-humanize": "1.0.0"
  },
  "bin": {
    "generateJsGivenReport": "./generateReport.js"
  },
  "files": [
    "generateReport.js",
    "index.js",
    "LICENSE",
    "README.md",
    "package.json"
  ],
  "ava": {
    "require": [
      "babel-core/register",
      "./spec/support/setup-tests.js"
    ],
    "babel": "inherit"
  },
  "engines": {
    "node": ">= 4.2.0"
  }
}
